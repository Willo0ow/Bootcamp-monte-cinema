      "target": "esnext",
      "module": "esnext",
      "moduleResolution": "node",
      "importHelpers": true,
      "isolatedModules": true,
      "noEmit": true,
      "baseUrl": ".",
      "jsx": "preserve",
      "paths": {
        "@assets": ["./src/assets"],
        "@components": ["./src/components"],
