{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "moduleResolution": "node",
      "importHelpers": true,
      "isolatedModules": true,
      "noEmit": true,
      "baseUrl": ".",
      "jsx": "preserve",
      "paths": {
        "@/*": ["./src/*"],
        "@components/*": ["./src/components/*"],
        "@helpers/*": ["./src/helpers/*"],
        "@assets/*": ["./src/assets/*"],
        "@icons/*": ["./src/components/icons/*"],
      }
    }
  }